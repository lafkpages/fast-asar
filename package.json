{
  "name": "fast-asar",
  "description": "A faster implementation of Electron's ASAR archive format.",
  "version": "1.0.5",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "devDependencies": {
    "@electron/asar": "^3.2.7",
    "bun-types": "latest",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@tybys/chromium-pickle-js": "^1.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun scripts/build.ts",
    "build:docs": "typedoc src/index.ts",
    "pretest": "bun scripts/cleanupTestData.ts",
    "test": "bun test",
    "prepublishOnly": "bun run build",
    "benchmark:list": "hyperfine -r 5 './node_modules/.bin/asar list test/ignore/app.asar' 'bun ./src/cli/index.ts list test/ignore/app.asar'"
  },
  "bin": "dist/cli/index.js",
  "optionalDependencies": {
    "ansi-colors": "^4.1.3"
  }
}
